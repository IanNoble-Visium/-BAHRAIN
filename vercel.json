{
  "version": 2,
  "name": "trucontext-bahrain-demo",
  "trailingSlash": false,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/publc/videos/(.*)\\.mp4",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Accept-Ranges", "value": "bytes" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/public/:path*", "destination": "/publc/:path*" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/docs", "dest": "/docs.html" }
  ]
}

