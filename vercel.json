{
  "version": 2,
  "name": "trucontext-bahrain-demo",
  "trailingSlash": false,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/public/videos/(.*).mp4",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Accept-Ranges", "value": "bytes" }
      ]
    },
    {
      "source": "/public/video-kpis/(.*).mp4",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Accept-Ranges", "value": "bytes" }
      ]
    }
  ],
  "rewrites": []
}

